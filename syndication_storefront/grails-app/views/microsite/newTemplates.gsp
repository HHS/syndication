<body>



<!-- html for classic template -->

<div class="container">

    <div class="row">

        <!-- col for centering -->
        <div class="col-xs-3"></div>

        <!-- col for content -->
        <div class="classic-builder col-xs-6">

            <div class="header">

                <a><div class="outlined">
                        <div class="section"><span>header</span></div>
                    </div></a>

            </div>

            <div class="content clearfix">

                <a><div class="outlined main">
                        <div class="section"><span>main content</span></div>
                    </div>
                </a>

                <a><div class="outlined top">
                        <div class="section"><span>sidebar<br/>one</span></div>
                    </div>
                    <a>

                <a><div class="outlined bottom">
                        <div class="section"><span>sidebar<br>two</span></div>
                    </div>
                </a>

            </div>

            <div class="footer">

                    <a><div class="outlined">
                        <div class="section"><span>footer</span></div>
                    </div>
                    </a>

            </div>

        </div>

        <!-- col for centering -->
        <div class="col-xs-3"></div>

    </div><!-- end row -->
        
</div><!-- end container -->

<!-- end html for classic template -->

